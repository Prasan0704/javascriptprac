def NumGen(n):
      
  
    for j in range(1, n+1):
  
       
        if j % 5 == 0 or j % 7 == 0:
            yield j

if __name__ == "__main__": 
        

    N = int(input("Enter N no.:"))
  
   
    for j in NumGen(N):
    print(j, end = " ")


X-----------------X-----------------X---------------------X------------------X-------------------X-------------------------X----------------X--------------------X----------------X



total = 0 
while True:
    a = input('Enter the no or press q to quit ')
    if a == 'q':
        break
    
    total += int(a)
    
print("total:",total)

